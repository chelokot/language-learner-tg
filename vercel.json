{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "outputDirectory": ".",
  "functions": {
    "api/bot-edge.ts": {
      "runtime": "edge",
      "regions": ["fra1", "arn1"],
      "maxDuration": 10,
      "includeFiles": "src/locales/**"
    },
    "api/**/*.{js,ts}": {
      "includeFiles": "src/locales/**"
    }
  }
}
